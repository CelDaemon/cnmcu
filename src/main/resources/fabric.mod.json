{
	"schemaVersion": 1,
	"id": "cnmcu",
	"version": "${version}",
	"name": "CodeNode Microcontrollers",
	"description": "Adds redstone microcontrollers powered by 6502 processors. Automate your projects in 6502 assembly!",
	"authors": [
		"elmfer"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "GPL-3.0-only",
	"icon": "assets/cnmcu/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.elmfer.cnmcu.CodeNodeMicrocontrollers"
		],
		"client": [
			"com.elmfer.cnmcu.CodeNodeMicrocontrollersClient"
		]
	},
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": "~${minecraft_version}",
		"java": ">=21",
		"fabric-api": "*"
	},
	"mixins": [
		{
			"environment": "client",
			"config": "cnmcu.mixin.json"
		}
	]
}